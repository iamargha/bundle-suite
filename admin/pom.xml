		<resources>
			<resource>
				<directory>src/main/webapp/META-INF/</directory>
				<filtering>false</filtering>
				<includes>
					<include>*.*</include>
				</includes>
			</resource>
			<resource>
				<directory>src/main/webapp/resources/</directory>
				<filtering>true</filtering>
				<includes>
					<include>*.*</include>
				</includes>
				<excludes>
					<exclude>*.properties</exclude>
					<exclude>*.xls</exclude>
				</excludes>
			</resource>
			<resource>
				<directory>src/main/webapp/resources/</directory>
				<filtering>false</filtering>
				<includes>
					<include>*.xls</include>
				</includes>
			</resource>
		</resources>
		<plugins>
			<plugin>
				<groupId>com.dbdeploy</groupId>
				<artifactId>maven-dbdeploy-plugin</artifactId>
				<version>3.0M3</version>
				<configuration>
					<changeLogTableName>oapi_changelog</changeLogTableName>
					<scriptdirectory>../src/main/resources/dbscript/deltas</scriptdirectory>
					<driver>oracle.jdbc.OracleDriver</driver>
					<url>${database.url}</url>
					<userid>${database.userid}</userid>
					<password>${database.password}</password>
					<dbms>ora</dbms>
					<delimiter>;</delimiter>
					<delimiterType>row</delimiterType>
					<outputfile>../src/main/resources/dbscript/${env}_scripts/consolidated_output_script_${maven.build.timestamp}.sql</outputfile>
					<undoOutputfile>../src/main/resources/dbscript/${env}_scripts/undo_${maven.build.timestamp}.sql</undoOutputfile>
				</configuration>
				<dependencies>
					<dependency>
						<groupId>oracle</groupId>
						<artifactId>ojdbc14</artifactId>
						<version>10.2.0.2.0</version>
					</dependency>
				</dependencies>
			</plugin>

			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-war-plugin</artifactId>
				<version>2.1.1</version>
				<configuration>
					<manifest>
						<addDefaultImplementationEntries>true</addDefaultImplementationEntries>
					</manifest>
					<archive>
						<manifestEntries>
							<Implementation-Title>${JOB_NAME}</Implementation-Title>
							<Implementation-Version>${BUILD_NUMBER}</Implementation-Version>
							<Implementation-Vendor-Id>${BUILD_ID}</Implementation-Vendor-Id>
							<Specification-Version>${BUILD_TAG}</Specification-Version>
							<Implementation-URL>${CVS_BRANCH}</Implementation-URL>
							<Specification-Title>${env.env}</Specification-Title>
							<Specification-Vendor>${env.tag}</Specification-Vendor>
						</manifestEntries>
					</archive>
					<webResources>
						<resource>
							<directory>src/main/webapp/WEB-INF</directory>
							<targetPath>WEB-INF</targetPath>
							<filtering>false</filtering>
							<includes>
								<include>*.xml</include>
								
							</includes>
						</resource>
						<resource>
							<directory>src/main/webapp/resources/</directory>
							<targetPath>WEB-INF/classes/</targetPath>
							<includes>
								<include>*.xml</include>
								<include>**/json/*</include>
							</includes>
						</resource>
						<resource>
							<directory>src/main/webapp</directory>
							<filtering>false</filtering>
							<includes>
								<include>**/*.jsp</include>
								<include>**/*.html</include>
							</includes>
						</resource>
					</webResources>
				</configuration>
			</plugin>
			<plugin>
					<groupId>org.apache.maven.plugins</groupId>
					<artifactId>maven-surefire-plugin</artifactId>
					<version>2.16</version>
					<configuration>
<!--
						<property>
							<name>listener</name>
							<value>com.mhe.connect.testng.util.TimeoutAnnotationTransformer</value>
						</property>
-->
						<threadCount>5</threadCount>
						<parallel>classes</parallel>
						<reportsDirectory>target/testng-surefire-reports/</reportsDirectory>
						<excludedGroups>cleanup</excludedGroups>
					</configuration>
				</plugin>
		</plugins>
	</build>
	<profiles>
		<profile>
			<id>wldeploy</id>
			<build>
				<plugins>
					<plugin>
						<groupId>org.apache.maven.plugins</groupId>
						<artifactId>maven-antrun-plugin</artifactId>
						<executions>
							<execution>
								<phase>install</phase>
								<goals>
									<goal>run</goal>
								</goals>
								<configuration>
									<tasks>
										<echo>Displaying value of hudson build properties</echo>
										<echo>cvs branch : ${CVS_BRANCH} == build tag : ${BUILD_TAG}
											== build number : ${BUILD_NUMBER}</echo>
										<echo>custom tag parameter : ${env.tag} -- custom env
											parameter : ${env.env}</echo>
										<taskdef name="wldeploy"
											classname="weblogic.ant.taskdefs.management.WLDeploy" />
										<!-- force an undeploy first. -->
										<echo>undeploy and deploy</echo>
										<wldeploy action="undeploy" verbose="true" debug="true"
											usenonexclusivelock="true" name="openapi_${env}" user="${bea.deploy.user}"
											password="${bea.deploy.password}" adminurl="${weblogic.server.url}"
											targets="${bea.target_server}" failonerror="false" />
										<wldeploy action="deploy" verbose="true" debug="true"
											upload="true" usenonexclusivelock="true" name="openapi_${env}"
											source="target/openAPI-war-${speech.version}.war" user="${bea.deploy.user}"
											password="${bea.deploy.password}" adminurl="${weblogic.server.url}"
											targets="${bea.target_server}" />
									</tasks>
								</configuration>

							</execution>
						</executions>
					</plugin>
				</plugins>
			</build>
		</profile>
		<profile>
			<id>dbdeploy</id>
			<build>
				<plugins>
