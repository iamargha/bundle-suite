		<!-- Swagger Implementation Start -->
		
		<profile>
			<id>swagger_documentation</id>
			<build>
				<plugins>
					<plugin>
						<groupId>com.github.kongchen</groupId>
						<artifactId>swagger-maven-plugin</artifactId>
						<version>2.1</version>
						<configuration>
							<apiSources>
								<apiSource>
									<locations>com.mhhe.openapi.rest;com.mhe.connect.services.gradebook.resource;com.mhe.connect.business.gradebook.valueobj;com.mhe.connect.services.gradebook.valueobj;com.mhhe.connect.openapi.vo.paam;com.mhe.connect.services.section</locations>
									<apiVersion>v1</apiVersion>
									<basePath>/openapi/jsp/swagger</basePath>
									<outputTemplate>wiki-new.mustache</outputTemplate>
									<outputPath>openAPI-war/src/main/webapp/jsp/swagger/strapdown.html</outputPath>
									<swaggerDirectory>openAPI-war/src/main/webapp/jsp/swagger</swaggerDirectory>
									<useOutputFlatStructure>true</useOutputFlatStructure>
								</apiSource>
							</apiSources>
						</configuration>
						<executions>
							<execution>
								<phase>compile</phase>
								<goals>
									<goal>generate</goal>
								</goals>
							</execution>
						</executions>
					</plugin>
				</plugins>
			</build>
		</profile>		
		
