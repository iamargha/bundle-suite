		<profile>
                     <id>copy_json</id>
                     <build>
                     <plugins>
                            <plugin>
                            <groupId>org.apache.maven.plugins</groupId>
                            <artifactId>maven-antrun-plugin</artifactId>
                            <version>1.6</version>
                            <executions>
                                  <execution>
                                  <phase>compile</phase>
                                 <goals>
                                         <goal>run</goal>
                                 </goals>
                                 <configuration>
                                         <tasks>
                                                <copy todir="target/classes/json" >
  													<fileset dir="src/main/webapp/jsp/swagger">
    												<include name="**/*.json" />
  													</fileset>
												</copy>
                                        </tasks>
